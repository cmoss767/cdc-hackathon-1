generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DB_ADDR")
}

model Trash {
    id       Int             @id @default(autoincrement())
    amount   Int?
    name     String?
    location Json?
    report   Boolean         @default(false)
    severity ReportSeverity?
}

enum ReportSeverity {
    LOW
    MEDIUM
    HIGH
}
